#
# The IceBox server endpoint configuration
#
IceBox.ServiceManager.Endpoints=tcp -h 0.0.0.0 -p 7778

#
# The IceStorm service
#
IceBox.Service.IceStorm=IceStormService,33:createIceStorm --Ice.Config=icestorm.config

Ice.Override.Timeout=5000
Ice.Override.ConnectTimeout=5000
Ice.RetryIntervals=-1

Ice.MessageSizeMax=5120

#
# Warn about connection exceptions
#
#Ice.Warn.Connections=1

#
# Network Tracing
#
# 0 = no network tracing
# 1 = trace connection establishment and closure
# 2 = like 1, but more detailed
# 3 = like 2, but also trace data transfer
#
#Ice.Trace.Network=1

#
# Protocol Tracing
#
# 0 = no protocol tracing
# 1 = trace protocol messages
#
#Ice.Trace.Protocol=1
